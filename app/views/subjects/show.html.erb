<div class="container">
<div class="alert alert-dark" role="alert">
  <a href="/subjects/new" class="alert-link">Create new subject</a>
</div>
<% @subjects.each do |s| %>
  <div class="alert alert-primary" role="alert">
    <p>
      Name: <%= s.name %>
    </p>
    <p>
      Description: <%= s.description  %>
    </p>
  </div>
<% end %>
</div>
